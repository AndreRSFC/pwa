{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "jsx": "react",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "pretty": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "allowJs": false,
    "lib": ["es2015", "es2016", "es2017", "dom"]
  }
}
