{
  "name": "pwa",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^4.9.1",
    "localforage": "^1.7.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "2.16.0",
    "react-emitter": "^1.0.4",
    "react-geolocated": "^2.3.0",
    "react-geolocation": "^1.0.3",
    "react-google-maps": "^9.4.5",
    "react-injector": "^0.2.1",
    "react-redux": "^5.0.6",
    "react-redux-firebase": "^2.0.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.27.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-js watch-css",
    "build-js": "react-scripts-ts build",
    "watch-js": "react-scripts-ts start",
    "build-css": "stylus -c src/styles/main.styl -u ./node_modules/rupture --out public/statics/styles",
    "watch-css": "npm run build-css && stylus -c -w src/styles/ -u ./node_modules/rupture --out public/statics/styles",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build": "npm-run-all build-js build-css",
    "deploy": "npm run-script build && firebase deploy --only hosting build"
  },
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.4",
    "npm-run-all": "^4.1.2",
    "rupture": "^0.7.1",
    "stylus": "^0.54.5",
    "typescript": "^2.9.2"
  }
}
